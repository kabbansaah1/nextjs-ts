{
  "extends": ["next/core-web-vitals", "prettier", "plugin:storybook/recommended"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "React"
      }
    ],
    "react/prop-types": [2],
    "react/require-default-props": [
      2,
      {
        "forbidDefaultForRequired": true,
        "functions": "defaultProps"
      }
    ]
  }
}
